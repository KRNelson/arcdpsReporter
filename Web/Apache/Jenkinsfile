pipeline {
    agent {
        docker {
            image 'node'
            args '-p 3443:3443'
        }
    }
    environment {
        CI = 'true'
        PATH = "${(tool 'docker')}/bin:${PATH}"
    }
    stages {
        stage('Build') {
            steps {
                sh 'npm install package.json'
            }
        }
    }
}